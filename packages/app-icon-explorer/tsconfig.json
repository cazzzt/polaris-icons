{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "module": "esnext"
  },
  "include": [
    "src"
  ]
}
